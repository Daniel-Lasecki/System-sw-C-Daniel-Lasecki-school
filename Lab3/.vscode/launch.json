{
    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "Debug with argv",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/out/build/Configure preset using toolchain file/fir_filter",  // Change to your actual binary
            "args": ["pressure_samples.csv", "filtered_results.csv","highpass"],           // Your argv arguments here
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": true,                        // optional, to use a terminal window
            "MIMode": "gdb",                                // or lldb if on mac
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}
